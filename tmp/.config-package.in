	menuconfig PACKAGE_iptables
		tristate "iptables............................... IPv4 firewall administration tool"
		default y if DEFAULT_iptables
		default m if ALL
		select PACKAGE_libip4tc
		select PACKAGE_libpthread if USE_EGLIBC
		select PACKAGE_libc
		select PACKAGE_librt if USE_EGLIBC
		select PACKAGE_libxtables
		select PACKAGE_kmod-ipt-core
		help
		 IPv4 firewall administration tool.
		 
		  Matches:
		   - icmp
		   - tcp
		   - udp
		   - comment
		   - limit
		   - mac
		   - multiport
		 
		  Targets:
		   - ACCEPT
		   - DROP
		   - REJECT
		   - LOG
		   - TCPMSS
		 
		  Tables:
		   - filter
		   - mangle
		 
		 http://netfilter.org/
		 OpenWrt Developers Team <openwrt-devel@openwrt.org>

